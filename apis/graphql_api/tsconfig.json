{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "lib": ["es2018", "dom"],
    "strict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "outDir": "dist",
    "sourceMap": true,
    "allowJs": true
  },
  "include": ["src/**/*.ts", "src/**/*.graphql"],
  "exclude": ["node_modules"]
}
